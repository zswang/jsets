{
  "name": "jsets",
  "version": "0.0.5",
  "description": "Define Getter and Setter",
  "homepage": "https://github.com/zswang/jsets",
  "author": {
    "name": "zswang",
    "url": "http://weibo.com/zswang"
  },
  "repository": "https://github.com/zswang/jsets.git",
  "license": "MIT",
  "main": "jsets.js",
  "files": [
    "jsets.js"
  ],
  "keywords": [
    "getter",
    "setter"
  ],
  "dependencies": {},
  "devDependencies": {
    "mocha": "2.0.1",
    "istanbul": "0.3.17",
    "should": "4.1.0",
    "uglify-js": "2.4.15",
    "jdists": "1.0.8",
    "jshint": "2.5.8"
  },
  "scripts": {
    "_update_version": "jdists version.jdists",
    "_dist": "jdists src/jsets.js -o jsets.js -r debug,test,remove,safe",
    "_compress": "uglifyjs jsets.js -o jsets.min.js -p 5 -c -m",
    "example": "jdists example.jdists.js -o test/example.js",
    "test": "istanbul cover --hook-run-in-context node_modules/mocha/bin/_mocha -- -R spec",
    "mocha": "jdists example.jdists.js -o test/example.js && mocha",
    "dist": "npm run _update_version && npm run example && npm run _dist && npm run _compress && npm run test",
    "lint": "jshint src/*.js *.json"
  }
}